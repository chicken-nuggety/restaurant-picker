<?xml version="1.1" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                           http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <changeSet author="Jayvan" id="1234">
        <createTable tableName="restaurant">
            <column autoIncrement="true" name="id" type="bigint">
                <constraints primaryKey="true"/>
            </column>
            <column name="name" type="varchar(20)">
            </column>
            <column name="active_status" type="boolean">
                <constraints nullable="false"/>
            </column>
            <column name="created_by" type="varchar(200)">
            </column>
            <column name="created_time" type="TIMESTAMP">
            </column>
            <column name="modified_by" type="varchar(200)">
            </column>
            <column name="modified_time" type="TIMESTAMP">
            </column>
        </createTable>
    </changeSet>
</databaseChangeLog>